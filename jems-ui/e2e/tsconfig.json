{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2021", "dom"],
    "types": ["cypress"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@fixtures/*": ["./cypress/fixtures/*"],
      "@commands/*": ["./cypress/support/*"]
    }
  },
  "include": ["**/*.ts"]
}
